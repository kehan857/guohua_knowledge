  "dependencies": {
    "vue": "^3.4.19",
    "vue-router": "^4.2.5",
    "ant-design-vue": "^4.1.2",
    "@ant-design/icons-vue": "^7.0.1",
    "echarts": "^5.4.3",
    "vue-echarts": "^6.6.3",

  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/tsconfig": "^0.5.1",
    "typescript": "~5.3.0",
    "vite": "^5.1.0",
    "vue-tsc": "^1.8.27",
    "less": "^4.2.0",
    "@types/node": "^20.11.17",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.20.1",
